<div class="container-fluid">
			<div class="span10">
				<nav class="navbar navbar-default" role="navigation">
					<div class="container-fluid">
						<div class="navbar-header">
						    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
						        <span class="sr-only">Toggle navigation</span>
						        <span class="icon-bar"></span>
						        <span class="icon-bar"></span>
						        <span class="icon-bar"></span>
						    </button>
						    <a class="navbar-brand" href="#">Bueller</a>
            </div>
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav">
                    <li><%= link_to "Home", controller: "students" %></li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                  <% if current_student %>
                    <p class="navbar-text current-student">
                      Hi <%= current_student.name %>! <%= link_to "Edit your profile", profile_path %>
                    </p>
                    <%= link_to "Sign out", sign_out_path, class: "btn btn-default navbar-btn"%></li>
                  <% else %>
                    <%= link_to "Sign up", new_student_path, class: "btn btn-default navbar-btn" %>
                    <%= link_to "Sign in", new_session_path, class: "btn btn-primary navbar-btn" %>
                  <% end %>
                </ul>
						</div>
          </div>
				</nav>
			</div>    
      <div class="row-fluid">
      <h1>Attendance for <%= Date.today %></h1>
        
        <div class="panel">
          <div>Quadrant 1</div>
          <ul>
            <% @in_seat_1.each do |student| %>
            <li><%= student.name %></li>
            <% end %>
          </ul>
        </div>
        
        <div>Quadrant 2</div>
        <ul>
          <% @in_seat_2.each do |student| %>
          <li><%= student.name %></li>
          <% end %>
        </ul>
        
        <div>Quadrant 3</div>
        <ul>
          <% @in_seat_3.each do |student| %>
          <li><%= student.name %></li>
          <% end %>
        </ul>
        
        <div>Quadrant 4</div>
        <ul>
          <% @in_seat_4.each do |student| %>
          <li><%= student.name %></li>
          <% end %>
        </ul>
        
        <%= link_to "Log Attendance", new_attendance_path, class: "btn btn-primary navbar-btn" %>
    </div>
  </body>
</html>